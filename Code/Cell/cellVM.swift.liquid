import UIKit

protocol {{ prefix }}{{ module_info.name }}CellVM {
    
    var title: String { get }
    var imageUrl: URL? { get }
}

class {{ prefix }}{{ module_info.name }}CellVMImp: {{ prefix }}{{ module_info.name }}CellVM {

    private var item: MItem
    
    init(_ item: MItem) {
        self.item = item
    }
    
    var title: String {
        return item.author
    }
    
    var imageUrl: URL? {
        guard let image = item.downloadUrl else {
            return nil
        }
        return URL(string: image)
    }
}

